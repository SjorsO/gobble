{
    "name": "sjorso/gobble",
    "description": "A Laravel specific wrapper around Guzzle that makes it easy to mock responses",
    "license": "MIT",
    "homepage": "https://github.com/SjorsO/gobble",
    "keywords": [
        "gobble",
        "guzzle",
        "laravel",
        "facade",
        "testing",
        "mock",
        "fake"
    ],
    "authors": [
        {
            "name": "Sjors Ottjes",
            "email": "sjorsottjes@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SjorsO\\Gobble\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SjorsO\\Gobble\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/container": "~5.5.0|~5.6.0|~5.7.0|~5.8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SjorsO\\Gobble\\Providers\\GobbleProvider"
            ],
            "aliases": {
                "Gobble": "SjorsO\\Gobble\\Facades\\Gobble"
            }
        }
    }
}
